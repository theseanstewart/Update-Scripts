{
    "name": "seanstewart/update-scripts",
    "description": "Allows you to run scripts to update your application, just like migrations",
    "keywords": ["updates", "migrations", "laravel", "laravel5"],
    "homepage": "https://github.com/theseanstewart/Plan-Config",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Stewart",
            "email": "sean@ekoim.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.2.*",
        "illuminate/support": "5.2.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Seanstewart\\UpdateScripts\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
